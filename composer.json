{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-translation-manager": "^0.5.1",
        "caouecs/laravel-lang": "^3.0",
        "chenhua/laravel5-markdown-editor": "^1.0",
        "chumper/zipper": "~1.0",
        "dingo/api": "2.0.0-alpha1",
        "elasticsearch/elasticsearch": "~6.0",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "flynsarmy/db-blade-compiler": "1.*",
        "hanson/vbot": "^2.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6.0",
        "league/html-to-markdown": "^4.6",
        "maatwebsite/excel": "^3.0",
        "matriphe/imageupload": "6.*",
        "mcamara/laravel-localization": "^1.3",
        "mews/purifier": "^2.1",
        "peinhu/aetherupload-laravel": "~1.0",
        "qcod/laravel-imageup": "^1.0",
        "radic/blade-extensions": "~7.1",
        "spatie/laravel-glide": "^3.2",
        "spatie/laravel-translatable": "^3.1",
        "tymon/jwt-auth": "^0.5.12",
        "vbot/blacklist": "^1.0",
        "vbot/express": "^1.0",
        "vbot/find-movies": "^1.1",
        "vbot/guess-number": "^1.0",
        "vbot/hot-girl": "^1.0",
        "vbot/http": "^1.0",
        "vbot/tuling": "^1.0",
        "overtrue/pinyin": "~4.0",
        "overtrue/wechat": "~4.1",
        "overtrue/easy-sms": "~1.1",
        "overtrue/laravel-follow": "~1.1",
        "overtrue/laravel-lang": "~3.0",
        "walkor/Workerman": "^3.5",
        "wpb/string-blade-compiler": "~3.6",
        "yuanchao/laravel-5-markdown-editor": "dev-master",
        "zedisdog/laravel-schema-extend": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-er-diagram-generator": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "orangehill/iseed": "^2.5",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
